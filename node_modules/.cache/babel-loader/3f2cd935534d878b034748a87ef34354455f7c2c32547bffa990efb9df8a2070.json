>Total: ₱{{ totalPrice }}</p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        products: [\r\n          { id: 1, name: 'Black Coffee', price: 39, quantity: 0 },\r\n          { id: 2, name: 'Espresso', price: 49, quantity: 0 },\r\n          { id: 3, name: 'Iced Latte', price: 59, quantity: 0 },\r\n          { id: 4, name: 'Americano', price: 69, quantity: 0 },\r\n          { id: 5, name: 'Cappuccino', price: 79, quantity: 0 },\r\n          { id: 6, name: 'Mocha', price: 89, quantity: 0 },\r\n          { id: 7, name: 'Caramel Macchiato', price: 99, quantity: 0 },\r\n          { id: 8, name: 'Matcha', price: 109, quantity: 0 },\r\n          { id: 9, name: 'Cold Brew', price: 119, quantity: 0 },\r\n          { id: 10, name: 'Frappe', price: 129, quantity: 0 },\r\n        ],\r\n        cart: []\r\n      };\r\n    },\r\n    computed: {\r\n      totalPrice() {\r\n        return this.cart.reduce((total, item) => {\r\n          return total + (item.product.price * item.quantity);\r\n        }, 0);\r\n      }\r\n    },\r\n    methods: {\r\n      addToCart(product) {\r\n        if (product.quantity > 0) {\r\n          const found = this.cart.find(item => item.product.id === product.id);\r\n          if (found) {\r\n            found.quantity += product.quantity;\r\n          } else {\r\n            this.cart.push({ product: product, quantity: product.quantity });\r\n          }\r\n          product.quantity = 0; // Reset quantity to zero after adding to cart\r\n        }\r\n      },\r\n      removeFromCart(index) {\r\n        this.cart.splice(index, 1);\r\n      },\r\n      updateQuantity(index) {\r\n        const quantity = parseInt(this.cart[index].quantity);\r\n        if (quantity <= 0) {\r\n          this.cart.splice(index, 1);\r\n        }\r\n      },\r\n      incrementQuantity(product) {\r\n        product.quantity++;\r\n      },\r\n      decrementQuantity(product) {\r\n        if (product.quantity > 0) {\r\n          product.quantity--;\r\n        }\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style>\r\n  .container {\r\n    display: flex;\r\n  }\r\n  \r\n  .menu {\r\n    flex: 1;\r\n    margin-right: 20px;\r\n  }\r\n  \r\n  .cart {\r\n    flex: 1;\r\n    margin-left: 20px;\r\n  }\r\n  \r\n  .product-list {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n  }\r\n  \r\n  .product {\r\n    margin: 10px;\r\n    padding: 10px;\r\n    border: 1px solid #ccc;\r\n  }\r\n  \r\n  .quantity-controls {\r\n    display: flex;\r\n    align-items: center;\r\n  }\r\n  \r\n  .quantity-controls button {\r\n    margin: 0 5px;\r\n  }\r\n  \r\n  .cart-item {\r\n    margin-bottom: 10px;\r\n  }\r\n  \r\n  .cart-info {\r\n    display: flex;\r\n    align-items: center;\r\n  }\r\n  \r\n  .remove-button {\r\n    margin-left: 10px;\r\n  }\r\n  </style>\r\n  "],"mappings":";AAqCE,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,CACR;QAAEC,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,cAAc;QAAEC,KAAK,EAAE,EAAE;QAAEC,QAAQ,EAAE;MAAE,CAAC,EACvD;QAAEH,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,UAAU;QAAEC,KAAK,EAAE,EAAE;QAAEC,QAAQ,EAAE;MAAE,CAAC,EACnD;QAAEH,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,YAAY;QAAEC,KAAK,EAAE,EAAE;QAAEC,QAAQ,EAAE;MAAE,CAAC,EACrD;QAAEH,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,WAAW;QAAEC,KAAK,EAAE,EAAE;QAAEC,QAAQ,EAAE;MAAE,CAAC,EACpD;QAAEH,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,YAAY;QAAEC,KAAK,EAAE,EAAE;QAAEC,QAAQ,EAAE;MAAE,CAAC,EACrD;QAAEH,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,OAAO;QAAEC,KAAK,EAAE,EAAE;QAAEC,QAAQ,EAAE;MAAE,CAAC,EAChD;QAAEH,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,mBAAmB;QAAEC,KAAK,EAAE,EAAE;QAAEC,QAAQ,EAAE;MAAE,CAAC,EAC5D;QAAEH,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,QAAQ;QAAEC,KAAK,EAAE,GAAG;QAAEC,QAAQ,EAAE;MAAE,CAAC,EAClD;QAAEH,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,WAAW;QAAEC,KAAK,EAAE,GAAG;QAAEC,QAAQ,EAAE;MAAE,CAAC,EACrD;QAAEH,EAAE,EAAE,EAAE;QAAEC,IAAI,EAAE,QAAQ;QAAEC,KAAK,EAAE,GAAG;QAAEC,QAAQ,EAAE;MAAE,CAAC,CACpD;MACDC,IAAI,EAAE;IACR,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,UAAUA,CAAA,EAAG;MACX